        <class_source><![CDATA[import java.util.*;
import java.lang.System.*;
import java.net.*;
import java.io.*;
import java.text.*;
import java.util.Date;
import java.util.Calendar;

import org.apache.commons.codec.binary.Base64;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException, Exception
{

    //First, get a row from the default input hop
    Object[] r = getRow();

    //If the row object is null, we are done processing.
	if (r == null) 
	{
      	setOutputDone();
    	return false;
    }

	String urlstring = get(Fields.In, "Url").getString(r);

	logBasic(urlstring);

	String result="";
	String errorMessage = "";
	String statusCode = "";
	String nextURL = "";

	//Making the Web Service call
    try 
	{
		String webPage = urlstring;//"https://ts75.mdsol.com/RaveWebServices/studies";
		String name = "ccalvo";
		String password = "password1";

		String authString = name + ":" + password;
		System.out.println("auth string: " + authString);
		byte[] authEncBytes = Base64.encodeBase64(authString.getBytes());
		String authStringEnc = new String(authEncBytes);

		URL url = new URL(webPage);
		URLConnection urlConnection = url.openConnection();
		urlConnection.setRequestProperty("Authorization", "Basic " + authStringEnc);
		InputStream is = urlConnection.getInputStream();
		InputStreamReader isr = new InputStreamReader(is,"UTF-8");

		statusCode = urlConnection.getHeaderField(0);
		logBasic("Status code: " + statusCode);

		if (statusCode.indexOf("200") > 0)
		{
			int numCharsRead;
			char[] charArray = new char[1024];

			StringBuffer sb = new StringBuffer();
			while ((numCharsRead = isr.read(charArray)) > 0) 
			{
				sb.append(charArray, 0, numCharsRead);
			}
			result = sb.toString();


			// Dealing with next link. Format: "<URL>; rel="next""
			nextURL = urlConnection.getHeaderField("Link");
			
			if (nextURL != null && nextURL.indexOf(">; rel=\"next\"") > 0)
			{
				nextURL = nextURL.substring(1, nextURL.indexOf(">;"));
			}
		}
		else
		{
			logBasic("Wrong status code");
		}

	} 
	catch (MalformedURLException e) 
	{
		errorMessage = e.toString();
	} 
	catch (IOException e) 
	{
		errorMessage = e.toString();
	}
        

	// Returning the Values

	get(Fields.Out, "ResponseBody").setValue(r, result);
	get(Fields.Out, "StatusCode").setValue(r, statusCode);
	get(Fields.Out, "ErrorMessage").setValue(r, errorMessage);
	get(Fields.Out, "NextUrl").setValue(r, nextURL);

	putRow(data.outputRowMeta, r);

	return true;
}

